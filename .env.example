# Example environment variables for JARS mobile app
SENTRY_DSN=https://public@sentry.example.com/1

# Base URL for your backend API
# Replace with your production endpoint
# Defaulting to deployed Railway API
EXPO_PUBLIC_API_BASE_URL=https://jars-cannabis-mobile-app-production.up.railway.app

# Optional CMS API for dynamic content
EXPO_PUBLIC_CMS_API_URL=https://cms.example.com

# Expo client settings
EXPO_PUBLIC_FIREBASE_API_KEY=
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=
EXPO_PUBLIC_FIREBASE_PROJECT_ID=
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=
EXPO_PUBLIC_FIREBASE_SENDER_ID=
EXPO_PUBLIC_FIREBASE_APP_ID=
EXPO_PUBLIC_FIREBASE_MEASUREMENTID=

# Node server settings
FIREBASE_PROJECT_ID=
FIREBASE_CLIENT_EMAIL=
FIREBASE_PRIVATE_KEY=
# JWT secret must be 32+ chars with lower/upper/number/symbol
JWT_SECRET=
# Global rate limiting (defaults shown for backend)
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=60
RATE_LIMIT_BLOCK_SECONDS=60
WEATHER_API_URL=https://api.openweathermap.org/data/2.5/weather
WEATHER_CACHE_TTL_MS=300000
EXTERNAL_API_TIMEOUT_MS=5000

# Stripe (replace with real credentials on launch)
STRIPE_PUBLISHABLE_KEY=
STRIPE_MERCHANT_ID=
EXPO_PUBLIC_OPENWEATHER_KEY=
